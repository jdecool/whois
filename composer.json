{
    "name": "jdecool/whois",
    "description": "PHP whois client implementation",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy DECOOL",
            "email": "contact@jdecool.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "JDecool\\Whois\\": "src"
        },
        "files": [
            "src/Cli/bootstrap.php",
            "src/Server/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JDecool\\Whois\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-sockets": "*",
        "christoph-kluge/reactphp-http-cors-middleware": "^2.0",
        "nikic/fast-route": "^1.3",
        "react/http": "^1.4",
        "spatie/dns": "^2.1",
        "symfony/console": "^5.3",
        "thecodingmachine/safe": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "seregazhuk/php-watcher": "dev-master"
    },
    "config": {
        "sort-packages": true
    }
}
